{
    "framework": "nextjs",
    "functions": {
        "src/app/api/webhooks/**/*.ts": {
            "maxDuration": 30
        },
        "src/app/api/ai/**/*.ts": {
            "maxDuration": 30
        },
        "src/app/api/tts/**/*.ts": {
            "maxDuration": 30
        }
    },
    "headers": [
        {
            "source": "/api/webhooks/(.*)",
            "headers": [
                {
                    "key": "Access-Control-Allow-Origin",
                    "value": "*"
                },
                {
                    "key": "Access-Control-Allow-Methods",
                    "value": "GET, POST, OPTIONS"
                }
            ]
        }
    ]
}